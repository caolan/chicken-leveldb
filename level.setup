;(compile -s "level.scm" -Iinclude -lleveldb -c++)

(compile -O2 -d1 -s "level.scm" -Iinclude -lleveldb -j "level" -c++)
(compile -O2 -d0 -s "level.import.scm")
(compile -O2 -d1 -c "level.scm" -Iinclude -unit "level" -o "level-static.o" -c++)

(install-extension
  'level
  '("level.so" "level.import.so" "level-static.o")
  '((version "3.5.3")
    (static "level-static.o")
    (documentation "level.html" "egg.jpg")))
